{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
        "@components/*": ["src/components/*"],
        "@layout/*": ["src/layout/*"],
        "@data/*": ["src/data/*"],
        "@scripts/*": ["src/scripts/*"]
    }
  }
}